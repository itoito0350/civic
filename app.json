{
  "expo": {
    "name": "civica",
    "slug": "civica",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "civic",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#4b6cb7"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#4b6cb7"
      },
      "edgeToEdgeEnabled": true
    },
   
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
